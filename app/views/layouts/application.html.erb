<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <title>Jaci Schneider-Harris</title>
  <%= stylesheet_link_tag 'application' %>
	<%= stylesheet_link_tag 'reset-min','fonts-min','grids-min' %>
     <%= javascript_include_tag 'application','jquery','jquery.tablesorter.min' %>
      <script type="text/javascript">
        <%= @content_for_page_scripts %>
      </script>  <%= csrf_meta_tag %>
</head>
<body>
	<div class="topbar">
		<div class="container">   
	
			<h3><%= link_to "Jaci Schneider-Harris", blogs_path, :method => 'index' %></h3>
			<ul>
	        	<li class="active"><%= link_to "Home", blogs_path, :method => 'index' %></li>
	        	<li><%= link_to "Resume", blogs_path, :method => 'index'%></li>
	       		<li><%= link_to "Portfolio", blogs_path, :method => 'index'%></li> 
	       		<li><%= link_to "Professional Goals", blogs_path, :method => 'index'%></li> 
	       		<li><%= link_to "Blog", blogs_path, :method => 'index'%></li> 
	
			</ul>
			<ul class = "secondary-nav">
				<li><%= link_to "Contact", blogs_path, :method => 'index'%></li> 
	       		
			</ul>				              
		</div>
	</div>

	<% flash.each do |name, msg| %>	
	 <%= content_tag :div, msg, :id => "flash_#{name}", :class => "alert-message"  %>
	 <% end %>

<%=yield %>
<%=yield :javascript %>
</body>
</html>
